Program tres_casas ;
var num1,num2,op:integer;


Begin
	writeln('escreva um numero:');
	readln(num1);

	writeln('escreva um numero:');
	readln(num2);
	
	writeln('escrava a operação a ser realizada')
	readln(op)
	
	case op of;
	*:writeln(num1*num2)
	/:writeln(num1/num2)
	+:writeln(num1+num2)

	
End.